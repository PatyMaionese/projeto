    /** Retorna o utm_source se existir */
    _utm_campaign : function() {
        if (this._isnull(this._qsp('utm_campaign'))) {
            return '';
        } else {
            return this._qsp('utm_campaign');
        } 
    },

    /** 
     * Retorna o referer, considerando os seguintes valores: 
        * organic : origem google, bing ou yahoo
        * direct : tráfego sem referer
        * same : tráfego interno, isto é, veio de outra página do próprio site
        * lomadee : tráfego de origem lomadee
        * buscape : tráfego de origem do buscapé
        * paid : tráfego pago de outras origens
     */
    _referrer : function() {
        var ref = document.referrer;
        if (this._isnull(ref)) {
            ref = '';
        }

        if (this._contains('lomadee.com', ref) || this._lmdsid()!='' || this._contains('lomadee', this._utm_source())) {
            return "lomadee";
        } else if ( (this._contains('google.com', ref) && this._gclid()=='') || this._contains('bing.com', ref) || this._contains('yahoo.com', ref)) {
            return "organic";
        } else if (this._gclid()!='') {
            return "google_paid";
        } else if (!this._isnull(this._utm_source()) && this._utm_source()!='') {
            return "paid";
        } else if (this._contains(this._domain(), ref)) {
            return "same";
        } else {
            return "direct";
        }
    },

    /** Configura a latência das lojas que são diferentes de 30 dias */
    _adv_latency : function() {
        if (_lmd_advertiser_id==6078) {
            return 7;
        } else {
            return 30;
