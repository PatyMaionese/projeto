var _lmd_core = {

    _isnull : function(obj) {
        if (typeof obj === 'undefined') {
            return true;
        } else if (obj==null) {
            return true;
        } else {
            return false;
        }
    },

    /** Retorna valor de parâmetro da querystring */
    _qsp : function(paramName) {
        if (this._isnull(paramName)) {
            return "";
        }

        var _lmd_match = RegExp('[?&]' + paramName + '=([^&]*)','i').exec(window.location.search);
	    return _lmd_match && decodeURIComponent(_lmd_match[1].replace(/\+/g, ' '));
    },

    /** Retorna o domínio raiz do site atual */
    _domain : function() {
        if (location.hostname.indexOf('www.')==0) {
            return location.hostname.replace('www.','');
        } else if (location.hostname.indexOf('shop.')==0) {
            return location.hostname.replace('shop.','');
        } else if (location.hostname.indexOf('lojas.')==0) {
            return location.hostname.replace('lojas.','');
        } else {
            return location.hostname;
        }
    },

    /** Cria elemento invisível no final do body da página */
    _celmt : function(url, id, type) {
        if (this._isnull(url)) {
            return false;
        }

        if (this._isnull(id)) {
            id = '_lmd_tk_' + new Date().getTime();
        }
        if (this._isnull(type)) {
            type = 'iframe';
        }
        
        var _lmd_elmt=document.createElement(type);
		_lmd_elmt.src=url;
		_lmd_elmt.id=id;							
		_lmd_elmt.style='display:none';
		_lmd_elmt.frameBorder = 0;
		_lmd_elmt.width = "0";
        _lmd_elmt.height = "0";
        
        document.getElementsByTagName('body')[0].appendChild(_lmd_elmt);
        
        return true;
    },

    /** Cria entrada no cookie */
    _cck : function(cookieName, value, domain, expiresDays, path) {
        if (this._isnull(cookieName) || this._isnull(value)) {
            return;
        }
        
        var cookieValue = cookieName + '=' + value;
        if (!this._isnull(domain)) {
            cookieValue += ';domain=' + domain;
        }
        if (!this._isnull(expiresDays)) {
            var d = new Date();
            d.setTime(d.getTime() + (expiresDays*24*60*60*1000));
            cookieValue += ';expires=' + d.toUTCString();
        }
        if (!this._isnull(path)) {
            cookieValue += ';path=' + path;
        }

        document.cookie = cookieValue;
    },

    /** Consulta entrada no cookie */
    _gck : function(cookieName) {
        var name = cookieName + "=";
        var decodedCookie = "";
        try{
            decodedCookie = decodeURIComponent(unescape(document.cookie));
        }catch(e){
            try{
                decodedCookie = decodeURIComponent(document.cookie);
            }catch(e2){}
        }
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    },
    
    _contains : function(target, data) {
        if (data.toLowerCase().indexOf(target.toLowerCase()) == -1) {
            return false;
        } else {
            return true;
        }
    },

    /** Retorna o lmdsid se existir */
    _lmdsid : function() {
