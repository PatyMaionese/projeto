/**
 * Este script define as regras para implementação da pixel cpa
 * 
 * Objetivos técnicos:
 *      - Montar e imprimir a pixel v1 e v2 de transação da lomadee (CPA, CPL e CPC)
 * 
 * Processos realizados:
 *      - Aguarda o carregamento do datalayer da lomadee
 *      - Verifica se a página em que o script esta sendo impresso representa a página de checkout
 *      - Imprime a pixel de transação da lomadee
 */

 /** Quantidade de tentativas realizadas para encontrar o datalayer */
 var _lmd_action_tentative = 0;

 function _lmd_action() {
    /** Número máximo de tentativas antes de desistir de encontrar o datalayer */
    var maxTentatives = 500;
    
    /** Tempo de espera antes de realizar mais 1 tentativa de encontrar o datalayer */
    var timeout = 150;

    /** Incrementa o número de tentativas realizadas */
    _lmd_action_tentative++;

    /** Verifica se não ultrapassou a quantidade máxima de tentativas */
    if (_lmd_action_tentative<maxTentatives) {
    
        /** Verifica se o datalayer não existe */
        if (typeof lomadee_datalayer == 'undefined' || lomadee_datalayer==null || _lmd_core._isnull(lomadee_datalayer)) {
        
            /** Realiza mais uma tentativa após periodo de timeout */
            setTimeout(_lmd_action, timeout);
            
        } else {
        
            /** Verifica se o datalayer não esta completo */
            if (_lmd_core._isnull(lomadee_datalayer.page)) {
            
                /** Realiza mais uma tentativa após periodo de timeout */
                setTimeout(_lmd_action, timeout);
                
            /** Verifica se página atual é a página de conversão */
            } else if (lomadee_datalayer.page=='conversion') {
            
            		                /*Se nao for girafa*/
	                if(_lmd_advertiser_id != 5727){
	                    try {
	                        _lmd_core._celmt('https://redir.lomadee.com/secure.html?utm_source=' + lmd_orig_traffic + '&utm_campaign=' + lmd_sid_traffic + '&utm_medium=' + location.hostname + '&utm_path=' + encodeURIComponent(location.pathname), 'pixel_analytics', 'iframe');
	                    } catch(e) {}
	                }
                
                /** Verifica se o atributo conversion não esta preenchido */
                if (_lmd_core._isnull(lomadee_datalayer.conversion)) {
                
                    /** Realiza mais uma tentativa após periodo de timeout */
                    setTimeout(_lmd_action, 100);
                    
                } else {
                
                    /** Imprime pixel de transação */
                    _lmd_action_print();
                    
                }
            }
        }
    } 
 }

/** Inicia o processo de validação do datalayer e impressão da pixel */
setTimeout(_lmd_action, 1000);

function _lmd_action_print() {
	if(_lmd_advertiser_id == 1){
		lomadee_datalayer.conversion.items[0].price = lomadee_datalayer.conversion.amount;
		lomadee_datalayer.conversion.items[0].quantity = 1;
	}
    /** Imprime pixel v2 */
    _printLomadeePixelV2();
}

function _printLomadeePixelV2() {
    /** Imprime image pixel da tag v2 */
    _lmd_core._celmt(
        "//secure.lomadee.com/v2.png?" + _lomadeeGetQueryStringV2(),
        "lmdcpa_v2",
        "img"
    );
}

/** Monta a querystring com os dados da transação */
function _lomadeeGetQueryStringV2() {
    var conversion = lomadee_datalayer.conversion;
    var currency = _lmd_core._gval(conversion.currency);
    var transaction = _lmd_core._gval(conversion.transactionId);
    var paymentType = _lmd_core._gval(conversion.paymentType);
    var type = _lmd_core._gval(lomadee_datalayer.type)=='' ? 'cpa' : lomadee_datalayer.type;
    var items = _lomadeeGetItensV2(conversion,type);
    var lmdsid = lmd_sid_traffic;
    var lmdcj = lmd_cj_traffic;
    var lmdorig = lmd_orig_traffic;
    
    /* Remove cookie do trafego */
    _lmd_core._cck('lmd_traf', '', _lmd_core._domain(), -1, "/");  
    
    return "lmdaId=" + _lmd_advertiser_id + 
        "&currency=" + currency + 
        "&paymentType=" + paymentType + 
        "&orderId=" + transaction + 
        items +
        "&type=" + type +
        "&lmdsid=" + lmdsid +
        "&lmdcj="+ lmdcj +
        "&lmdorig="+ lmdorig +
        "&origin=lmd" +
        "&lmdtraf="+encodeURIComponent(lmd_last_traffic);
}

function _lomadeeGetItensV2( conversion, type){
	var discountItem = 0;
	if(conversion.hasOwnProperty('discount')){
		var qtdTotItens = 0;
		for (var cont=0; cont<conversion.items.length; cont++){
			qtdTotItens += conversion.items[cont].quantity;
		}
		
		discountItem = conversion.discount/qtdTotItens;
		
	}
	
	return _lomadeeItensToProd(conversion,discountItem, type);
	
}

function _lomadeeItensToProd( conversion, discountItem, type ){
	var prods = '';
	/** Realiza o processo para geração da pixel CPA */
    if (type=='cpa') {
		var items = _lmd_core._gval(conversion.items);
		for(var i = 0; i < items.length; i++){
			prods += '&prod=' + 
					 encodeURIComponent(items[i].sku) + ';' +
					 encodeURIComponent(items[i].category) + ';' +
					 encodeURIComponent(items[i].price - discountItem) + ';' +
					 encodeURIComponent(items[i].quantity) + ';' +
					 encodeURIComponent(items[i].name);
		}
	/** Realiza o processo para geração da pixel CPL */
    } else if (type=='cpl') {
		prods += '&prod=' + conversion.transactionId + ';'+conversion.eventId+';0;1;LEAD';
	/** Realiza o processo para geração da pixel CPL */
    } else if (type=='cpc') {
        /** Ainda não esta sendo utilizado e precisa ser implementado */
    }
	
	return prods;
}

