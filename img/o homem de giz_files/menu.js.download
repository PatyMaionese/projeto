		var menuTimer = setTimeout(function(){});
		var msMenuHover = 300;
		$('.first-menu .toggle-menu').hover(function(){
			var target = $(this).find('.menu-content-container');
			var link = $(this).find('.toggle-menu-link');
			
			clearTimeout(menuTimer);
			
			if(target){
				menuTimer = setTimeout(function(){
					$('.menu-content-container').hide();
					$('.toggle-menu-link').removeClass('active');
					$(target).show();
					$(link).addClass('active');
					$("#bgMenuHover").show();
				}, msMenuHover);
			}
		}, function(){
			var target = $(this).find('.menu-content-container');
			
			if(target){
				clearTimeout(menuTimer);
				menuTimer = setTimeout(function(){
					$('.menu-content-container').hide();
					$('.toggle-menu-link').removeClass('active');
					$("#bgMenuHover").hide();
				}, msMenuHover);
			}
		});

		$('.logo-menu').hover(function(){
			$('.first-menu .menu-lists').addClass('active');
			$("#bgMenuHover").show();
		}, function(){
			
			setTimeout(function(){
				if($('.first-menu .menu-lists').is(':hover')){
					$('.first-menu .menu-lists').hover(undefined, function(){
						$('.first-menu .menu-lists').removeClass('active');
						$("#bgMenuHover").hide();
					});
				} else {
					$('.first-menu .menu-lists').removeClass('active');
					$("#bgMenuHover").hide();
				}
			}, 300);
		});
		
	}, 200);
	
